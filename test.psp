-- simple code in py_simplifier

-- imp is import
imp math 

-- Simple hello world
prl "Hello world"

-- you dont need () if you dont use it
fun main:+ 
	open* "test.txt", "w" as file:+
		file.write("lol")

-- if you need () you can use it
fun test(number):+ 
	re* number * 2

-- this is 'if "__main__" == __name__:'
ifmain* 
	prl "hi"
	-- or for print you can use
	prl"hi"

	prl math.pi
	prl test(2)
	main()
